{
  "name": "geomoment",
  "version": "0.6.1",
  "description": "moment-timezone, if moment-timezone forced you to choose a particular moment in time. also, some extra sugar.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script test/**/*.coffee",
    "build": "coffee --bare --compile --output lib/ src/ && npm run patchMoment && npm run document",
    "patchMoment": "coffee util/patch_moment.coffee",
    "document": "coffee util/document_date_formats.coffee",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodeggs/geomoment.git"
  },
  "keywords": [
    "moment",
    "date",
    "time",
    "timezone",
    "tz",
    "moment-timezone"
  ],
  "author": "Max Edmands <git@maxedmands.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goodeggs/geomoment/issues"
  },
  "devDependencies": {
    "moment": "2.7.0",
    "moment-timezone": "^0.1.0",
    "falafel": "^0.3.1",
    "mocha": "~1.15.1",
    "coffee-script": "~1.6.3",
    "chai": "~1.8.1"
  }
}
